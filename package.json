{
  "name": "alqefaritreern-expo",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "expo start --ios --android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "check-format": "prettier --check 'src/**/*.{js,jsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "echo 'No tests configured yet'",
    "check-all": "npm run lint && npm run check-format",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "submit:ios": "eas submit --platform ios --latest",
    "deploy": "eas build --platform ios --profile production --auto-submit",
    "deploy:preview": "eas build --platform ios --profile preview"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.2.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@supabase/supabase-js": "^2.56.1",
    "d3-hierarchy": "^3.1.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "expo": "~53.0.22",
    "expo-asset": "^11.1.7",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "^7.1.5",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "^14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-sharing": "^13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "lottie-react-native": "7.2.2",
    "moment": "^2.30.1",
    "moment-hijri": "^3.0.0",
    "moti": "^0.30.0",
    "nativewind": "^4.1.23",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-animatable": "^1.4.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "3.17.5",
    "react-native-shadow-2": "^7.1.2",
    "react-native-svg": "15.11.2",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@types/react": "~19.0.10",
    "eas-cli": "^16.19.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.8.3"
  },
  "private": true
}
