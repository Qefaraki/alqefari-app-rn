{
  "name": "alqefaritreern-expo",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "start:clean": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "check-format": "prettier --check 'src/**/*.{js,jsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest --config jest.backend.config.js",
    "test:backend:watch": "jest --config jest.backend.config.js --watch",
    "test:backend:coverage": "jest --config jest.backend.config.js --coverage",
    "test:all": "npm run test && npm run test:backend",
    "check-all": "npm run lint && npm run check-format",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "submit:ios": "eas submit --platform ios --latest",
    "deploy": "eas build --platform ios --profile production --auto-submit",
    "deploy:preview": "eas build --platform ios --profile preview",
    "update:preview": "eas update --channel preview",
    "update:production": "eas update --channel production",
    "update:list": "eas update:list",
    "update:rollback": "eas update:rollback",
    "update:view": "eas channel:view production",
    "postinstall": "npx patch-package || true",
    "dev:reset": "node scripts/fix-metro.js && npm run start:clean",
    "dev:rebuild": "node scripts/fix-metro.js && npx expo run:ios --clear",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
