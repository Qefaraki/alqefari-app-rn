{
  "expo": {
    "name": "Alqefari - أسرة القفاري",
    "slug": "alqefari-family-tree",
    "version": "2.0.0",
    "owner": "alqefari",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9F7F3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "1.0.0",
    "ios": {
      "bundleIdentifier": "com.Alqefari.alqefari",
      "supportsTablet": true,
      "buildNumber": "44",
      "infoPlist": {
        "NSCameraUsageDescription": "تحتاج التطبيق إلى الوصول إلى الكاميرا لالتقاط صور الملف الشخصي",
        "NSPhotoLibraryUsageDescription": "تحتاج التطبيق إلى الوصول إلى مكتبة الصور لاختيار صور الملف الشخصي",
        "UISupportsDocumentBrowser": true,
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleDevelopmentRegion": "ar",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.Alqefari.alqefari",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "alqefari",
    "plugins": [
      [
        "expo-router",
        {
          "sitemap": false
        }
      ],
      "expo-dev-client",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#A13333",
          "sounds": []
        }
      ],
      "expo-video"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#A13333",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} إشعارات جديدة"
    },
    "extra": {
      "eas": {
        "projectId": "7503aa5d-8e0a-48ce-9621-3a11303cad17"
      }
    }
  }
}
